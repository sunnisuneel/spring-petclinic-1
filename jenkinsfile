pipeline {
    agent { 'qt' }
    stages {
        stage('git') {
            steps {
                git branch: 'main', 
              url: 'https://github.com/sunnisuneel/spring-petclinic-1.git'
            }
        }
        stage('build') {
            steps {
                sh 'mvn package'
            }
        }
    }

}